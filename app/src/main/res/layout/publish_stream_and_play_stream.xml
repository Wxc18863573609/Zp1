<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:keepScreenOn="true"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.zupu.zp.testpakeyge.TitleLayout
            android:id="@+id/title"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:name="通话中">

            <Button
                android:id="@+id/goBackToVideoCommunicationInputRoomidUI"
                android:onClick="goBackToVideoCommunicationInputRoomidUI"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:background="@mipmap/ic_launcher" />


        </com.zupu.zp.testpakeyge.TitleLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <RelativeLayout
                    android:layout_marginLeft="@dimen/dp_40"
                    android:layout_marginTop="@dimen/dp_30"
                    android:background="#fff"
                    android:layout_width="0dp"
                    android:layout_height="0dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="对方名称"
                        android:id="@+id/targetname"
                        />

                    <ImageView
                        android:layout_toRightOf="@+id/targetname"
                        android:layout_width="@dimen/dp_50"
                        android:layout_height="@dimen/dp_50"
                        android:src="@drawable/zhanwei"
                        android:id="@+id/tagethead"
                        />
                </RelativeLayout>

               <RelativeLayout
                   android:layout_width="match_parent"
                   android:id="@+id/rl_remote"
                   android:layout_height="match_parent">
                   <LinearLayout
                       android:background="#666666"
                       android:orientation="vertical"
                       android:layout_width="match_parent"
                       android:layout_height="match_parent"
                       android:id="@+id/ll_view_container_0"
                       >

                   </LinearLayout>

                   <RelativeLayout
                       android:layout_width="wrap_content"
                       android:id="@+id/reyaoqing"
                       android:layout_centerHorizontal="true"
                       android:layout_alignParentBottom="true"
                       android:layout_marginTop="@dimen/dp_5"
                       android:layout_height="wrap_content">
                       <LinearLayout
                           android:id="@+id/userliner"
                           android:layout_marginBottom="@dimen/dp_350"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content">
                           <RelativeLayout
                               android:layout_width="@dimen/dp_150"
                               android:orientation="vertical"
                               android:layout_height="wrap_content">
                               <TextView
                                   android:layout_alignParentRight="true"
                                   android:layout_marginRight="@dimen/dp_13"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:id="@+id/username"
                                   android:layout_centerHorizontal="true"
                                   android:layout_marginTop="@dimen/dp_5"
                                   android:text="王医生"
                                   android:layout_marginLeft="@dimen/dp_5"
                                   />
                               <TextView
                                   android:layout_below="@+id/username"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:layout_centerHorizontal="true"
                                   android:layout_marginTop="@dimen/dp_5"
                                   android:text="正在等待对方接受邀请"
                                   android:layout_marginLeft="@dimen/dp_5"
                                   />
                           </RelativeLayout>
                           <ImageView
                               android:layout_width="@dimen/dp_50"
                               android:layout_height="@dimen/dp_50"
                               android:id="@+id/head"
                               android:src="@drawable/ic_launcher_background"
                               />


                       </LinearLayout>

                       <ImageView
                           android:layout_below="@+id/userliner"
                           android:layout_centerHorizontal="true"
                           android:layout_marginTop="@dimen/dp_15"
                           android:layout_width="@dimen/dp_50"
                           android:layout_height="@dimen/dp_50"
                           android:id="@+id/call"
                           android:src="@drawable/qx"
                           />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_below="@id/call"
                           android:layout_centerHorizontal="true"
                           android:layout_marginTop="@dimen/dp_5"
                           android:text="取消"
                           android:layout_marginBottom="@dimen/dp_10"
                           android:layout_marginLeft="@dimen/dp_5"
                           />
                   </RelativeLayout>

                   <RelativeLayout
                       android:layout_width="wrap_content"
                       android:id="@+id/rejie"
                       android:visibility="gone"
                       android:layout_centerHorizontal="true"
                       android:layout_alignParentBottom="true"
                       android:layout_marginTop="@dimen/dp_5"
                       android:layout_height="wrap_content">

                       <LinearLayout
                           android:id="@+id/user2liner"
                           android:layout_marginTop="@dimen/dp_150"
                           android:layout_marginLeft="@dimen/dp_30"
                           android:layout_marginBottom="@dimen/dp_320"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content">
                           <RelativeLayout
                               android:layout_width="@dimen/dp_150"
                               android:orientation="vertical"
                               android:layout_height="wrap_content">
                               <TextView
                                   android:layout_alignParentRight="true"
                                   android:layout_marginRight="@dimen/dp_13"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:id="@+id/user2name"
                                   android:layout_centerHorizontal="true"
                                   android:layout_marginTop="@dimen/dp_5"
                                   android:text="王医生"
                                   android:layout_marginLeft="@dimen/dp_5"
                                   />
                               <TextView
                                   android:layout_below="@+id/user2name"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:layout_centerHorizontal="true"
                                   android:layout_marginTop="@dimen/dp_5"
                                   android:text="是否接受邀请"
                                   android:layout_marginLeft="@dimen/dp_5"
                                   />
                           </RelativeLayout>
                           <ImageView
                               android:layout_width="@dimen/dp_50"
                               android:layout_height="@dimen/dp_50"
                               android:id="@+id/head2"
                               android:src="@drawable/ic_launcher_background"
                               />


                       </LinearLayout>

                       <RelativeLayout
                           android:layout_below="@+id/user2liner"
                           android:layout_marginLeft="@dimen/dp_40"
                           android:layout_alignParentBottom="true"
                           android:id="@+id/lefts"
                           android:layout_width="wrap_content"
                           android:orientation="vertical"
                           android:layout_height="wrap_content">



                           <ImageView
                               android:layout_centerHorizontal="true"
                               android:layout_marginTop="@dimen/dp_15"
                               android:layout_width="@dimen/dp_50"
                               android:layout_height="@dimen/dp_50"
                               android:id="@+id/jieting"
                               android:src="@drawable/call"
                               />

                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_alignParentBottom="true"
                               android:layout_below="@id/jieting"
                               android:layout_centerHorizontal="true"
                               android:layout_marginTop="@dimen/dp_5"
                               android:text="接听"
                               android:layout_marginBottom="@dimen/dp_10"
                               />
                       </RelativeLayout>
                       <RelativeLayout
                           android:layout_marginLeft="@dimen/dp_40"
                           android:layout_below="@+id/user2liner"
                           android:layout_toRightOf="@+id/lefts"
                           android:id="@+id/right"
                           android:layout_width="wrap_content"
                           android:orientation="vertical"
                           android:layout_height="wrap_content">
                           <ImageView
                               android:layout_marginTop="@dimen/dp_15"
                               android:layout_centerHorizontal="true"
                               android:layout_marginLeft="@dimen/dp_90"
                               android:layout_width="@dimen/dp_50"
                               android:layout_height="@dimen/dp_50"
                               android:id="@+id/guaduan"
                               android:src="@drawable/qx"
                                />
                           <TextView
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_below="@id/guaduan"
                               android:layout_centerHorizontal="true"
                               android:layout_marginTop="@dimen/dp_5"
                               android:text="挂断"
                               android:layout_marginBottom="@dimen/dp_10"
                               />
                       </RelativeLayout>
                   </RelativeLayout>
               </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_local"
                    android:layout_width="@dimen/dp_200"
                    android:layout_height="@dimen/dp_200">
                    <LinearLayout
                        android:background="#666666"
                        android:orientation="vertical"
                        android:layout_height="match_parent"
                        android:id="@+id/ll_view_container_1"
                        android:layout_width="match_parent">
                    </LinearLayout>


                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="-100dp"
                android:id="@+id/gua"
                android:background="#00000000"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/dp_90"
                    android:layout_width="@dimen/dp_50"
                    android:layout_height="@dimen/dp_50"
                    android:id="@+id/guaimg"
                    android:src="@drawable/qx"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/guaimg"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/dp_5"
                    android:text="挂断"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="-100dp"
                android:id="@+id/gua1"
                android:background="#00000000"
                android:layout_marginLeft="@dimen/dp_110"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/dp_90"
                    android:layout_width="@dimen/dp_50"
                    android:layout_height="@dimen/dp_50"
                    android:id="@+id/guaimg1"
                    android:src="@drawable/qx"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/guaimg1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/dp_5"
                    android:text="挂断"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:layout_marginLeft="@dimen/dp_32"
                android:id="@+id/retime"
                android:layout_marginTop="-350dp"
                android:visibility="gone"
                android:layout_centerHorizontal="true"
                android:layout_width="@dimen/dp_300"
                android:layout_height="@dimen/dp_200">
                <TextView
                    android:layout_marginLeft="@dimen/dp_100"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="用户"
                    android:layout_marginTop="@dimen/dp_12"
                    android:id="@+id/texta"
                    />
                <ImageView
                    android:layout_toRightOf="@id/texta"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:id="@+id/imga"
                    android:layout_width="@dimen/dp_40"
                    android:layout_height="@dimen/dp_40"
                    android:src="@drawable/ic_launcher_background"
                    />


                <TextView
                    android:layout_centerHorizontal="true"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="0"
                    android:id="@+id/tv_countdowntimer"
                    android:gravity="center"
                    />
            </RelativeLayout>

            <LinearLayout
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="摄像头:"/>
                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:id="@+id/CameraState"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="麦克风:"/>
                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="true"
                    android:id="@+id/MicrophoneState"/>

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>